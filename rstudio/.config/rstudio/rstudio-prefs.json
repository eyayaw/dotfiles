{
    "save_workspace": "never",
    "load_workspace": false,
    "initial_working_directory": "~",
    "remove_history_duplicates": true,
    "restore_last_project": false,
    "insert_native_pipe_operator": true,
    "continue_comments_on_newline": true,
    "highlight_selected_line": true,
    "scroll_past_end_of_document": true,
    "highlight_r_function_calls": true,
    "rainbow_parentheses": true,
    "auto_append_newline": true,
    "strip_trailing_whitespace": true,
    "auto_save_on_blur": true,
    "show_help_tooltip_on_idle": true,
    "warn_variable_defined_but_not_used": true,
    "default_encoding": "UTF-8",
    "console_double_click_select": true,
    "font_size_points": 13,
    "rmd_viewer_type": "pane",
    "posix_terminal_shell": "zsh",
    "custom_shell_command": "/bin/zsh",
    "show_last_dot_value": true,
    "jobs_tab_visibility": "shown",
    "visual_markdown_editing_show_margin": true,
    "syntax_color_console": true,
    "show_rmd_render_command": true,
    "default_project_location": "~/Developer",
    "save_files_before_build": true,
    "check_unexpected_assignment_in_function_call": true,
    "show_doc_outline_rmd": true,
    "doc_outline_show": "sections_and_chunks",
    "limit_visible_console": true,
    "rmd_chunk_output_inline": false,
    "check_arguments_to_r_function_calls": true,
    "rmd_auto_date": true,
    "document_author": "Eyayaw Beze",
    "reduced_motion": true,
    "visual_markdown_code_editor_line_numbers": true,
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "TabSet2",
            "Console"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": false,
        "additional_source_columns": 0
    },
    "reindent_on_paste": false,
    "auto_detect_indentation": true,
    "focus_console_after_exec": true,
    "disable_renderer_accessibility": true,
    "soft_wrap_r_files": true,
    "indent_guides": "rainbowfills",
    "copilot_indexing_enabled": true,
    "copilot_completions_delay": 500,
    "copilot_enabled": true,
    "graphics_backend": "ragg",
    "editor_theme": "Solarized Light",
    "show_terminal_tab": false
}